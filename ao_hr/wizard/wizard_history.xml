<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_employee_history_wizard_form" model="ir.ui.view">
        <field name="name">hr.employee.history.wizard.form</field>
        <field name="model">hr.employee.history.wizard</field>
        <field name="arch" type="xml">
           <form string="Histórico de Funcionário">
           
                            <field name="contract_id"/>
                            <field name="employee_id" required="1" placeholder="Nome do funcionário."
                                   options="{'no_quick_create': True, 'no_create_edit': True}"
                            />
                <footer>
                    <button name="action_print_history" string="Imprimir" type="object" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
           </form>  
        </field>
  </record>
</odoo>
