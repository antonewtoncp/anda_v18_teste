<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_employee_number" model="ir.sequence">
            <field name="name">Employee number sequence</field>
            <field name="code">hr.employee</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_contract_number" model="ir.sequence">
            <field name="name">Contract sequence</field>
            <field name="code">hr.contract</field>
            <field name="prefix">CD/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
