<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_hr_inss_configuration_tree_view" model="ir.ui.view">
        <field name="name">hr.inss.config.tree</field>
        <field name="model">hr.inss.config</field>
        <field name="priority" eval="100"/>
        <field name="arch" type="xml">
            <tree string="INSS Configuration" editable="bottom">
                <field name="name" readonly="1"/>
                <field name="tax" required="1"/>
                <field name="inss_type" required="1"/>
                <field name="create_date" readonly="1"/>
                <field name="write_date" readonly="1"/>
            </tree>

        </field>
    </record>

    <record id="action_hr_inss_configuration" model="ir.actions.act_window">
        <field name="name">INSS Configuration</field>
        <field name="res_model">hr.inss.config</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_hr_inss_configuration_tree_view"/>
    </record>
</odoo>